require(['text!placestemplate.html', 'jquery-1.7.1', 'underscore', 'mustache'], function(placesTemplate){
	function populatePlaces(){
		$.when($.get('/currentPlace', $.get('/places')).done(function(args1, args2){
			
		});
	}

	$('#countplaces').click(function(e){
		e.preventDefault();
		populatePlaces();
	});

	populatePlaces();	
});
